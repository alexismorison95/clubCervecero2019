    <FrameLayout
        android:id="@+id/container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="60dp">

    </FrameLayout>

    <include layout="@layout/bottom_bar"/>

</androidx.coordinatorlayout.widget.CoordinatorLayout>